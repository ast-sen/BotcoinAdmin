{
  "expo": {
    "sdkVersion": "54.0.0",
    "name": "Botcoin Admin",
    "slug": "botcoin-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.botcoin.admin",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access for scanning QR codes",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access for uploading images",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.botcoin.admin",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b0c4965-d234-40be-9b58-cd44bf617790"
      }
    }
  }
}
